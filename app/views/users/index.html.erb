<div>
  <h2>Users</h2>
  <table>
    <thead>
      <tr>
        <th>Image</th>
        <th>Name</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
       <% @users.each do |users| %>
         <tr>
           <td><%= image_tag users.get_profile_image(80,80) %></td>
           <td><%= users.name %></td>
           <td><%= link_to "Show", user_path(users.id) %></td>
      <% end %>
    </tbody>
  </table>
</div>
